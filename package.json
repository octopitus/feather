{
  "name": "feather",
  "version": "0.1.0",
  "description": "document & note for developers",
  "scripts": {
    "clean": "rimraf dist",
    "build:webpack": "NODE_ENV=production webpack -p --config webpack.config.prod.js",
    "build": "npm run clean && npm run build:webpack",
    "start": "NODE_ENV=development node bin/dev.js",
    "test": "NODE_ENV=testing ava --fail-fast --verbose --require=babel-register --require=css-modules-require-hook",
    "lint": "eslint src --ext .js",
    "lint:fix": "eslint src --ext .js --fix"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/octopitus/feather.git"
  },
  "keywords": [
    "react",
    "note",
    "document"
  ],
  "author": "Luy Tran <thailuy86@gmail.com>",
  "license": "BSD-3-Clause",
  "bugs": "https://github.com/octopitus/feather/issues",
  "homepage": "https://github.com/octopitus/feather",
  "devDependencies": {
    "autoprefixer": "^6.3.3",
    "ava": "^0.16.0",
    "babel-core": "^6.9.0",
    "babel-eslint": "^7.0.0",
    "babel-loader": "^6.2.4",
    "babel-plugin-react-transform": "^2.0.2",
    "babel-plugin-transform-runtime": "^6.9.0",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-0": "^6.5.0",
    "babel-register": "^6.9.0",
    "babel-runtime": "^6.9.0",
